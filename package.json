{
  "name": "cypressqaautomation",
  "version": "1.0.0",
  "description": "This a test automation framework with Cypress, Cucumber, Gherkin and JavaScript.",
  "main": "index.js",
  "scripts": {
    "start": "serve --listen ${PORT:-3000}",
    "test": "npm run scripts || npm run posttest",
    "cy:open": "cypress open",
    "cy:record:run": "cypress run --record --key 663fb42f-f5f5-4a93-9212-c3aeb66db140",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "clean:reports": "rmdir /s /q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "clean:videos": "rmdir /s /q cypress\\videos && mkdir cypress\\videos",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run --browser chrome",
    "combine-reports": "npx mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/testResultReport.json",
    "generate-report": "npx marge cypress/reports/testResultReport.json --reportDir ./ --inline ",
    "posttest": "npm run combine-reports && npm run generate-report",
    "wdio": "wdio run ./wdio.conf.js"
  },
  "keywords": [
    "cypress",
    "automation",
    "test",
    "cases",
    "javascript",
    "use",
    "cases",
    "smoke"
  ],
  "author": "qaEnginner",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.181",
    "@types/lodash-es": "^4.17.6",
    "cucumber": "^6.0.7",
    "cypress-get-table": "^1.0.1",
    "dayjs": "^1.11.2",
    "gherkin": "^9.0.0",
    "jquery-mobile": "^1.5.0-alpha.1",
    "prettier": "^2.6.1"
  },
  "devDependencies": {
    "@cypress/instrument-cra": "^1.4.0",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/simonsmith__cypress-image-snapshot": "npm:@types/cypress-image-snapshot@^3.1.6",
    "@wdio/cli": "^8.1.3",
    "@wdio/local-runner": "^8.1.3",
    "@wdio/mocha-framework": "^8.1.2",
    "@wdio/spec-reporter": "^8.1.2",
    "appium": "^2.0.0-beta.52",
    "appium-uiautomator2-driver": "^2.12.2",
    "child_process": "^1.0.2",
    "chromedriver": "^109.0.0",
    "cucumber": "^6.0.7",
    "cypress": "^11.1.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-multi-reporters": "^1.6.2",
    "driver": "^1.0.0",
    "gherkin": "^9.0.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0",
    "react-scripts": "^5.0.1",
    "selenium-webdriver": "^4.8.0",
    "serve": "^14.2.0",
    "uiautomator2": "^1.0.2",
    "wd": "^1.14.0",
    "wdio-chromedriver-service": "^8.0.1",
    "wdio-selenium-standalone-service": "^0.0.12",
    "webdriverio": "^8.2.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ioanaPrv/cypressFramework.git"
  },
  "bugs": {
    "url": "https://github.com/ioanaPrv/cypressFramework/issues"
  },
  "homepage": "https://github.com/ioanaPrv/cypressFramework#readme"
}
